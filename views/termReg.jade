extends layout

block content
    .container
        h1 端末登録
        form(role='form', action="/term/register",method="post", style='max-width: 300px;')
            .form-group
                input.form-control(type='text', name="_id")
            .form-group
                input.form-control(type='pw', name="pw")
            button.btn.btn-default(type='submit') Submit
        a(href='/')
        button.btn.btn-primary(type="button") Cancel